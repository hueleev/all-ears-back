<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "WEB-INF/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.hyungwon.test.TestDAO">
    <select id="getBoardDtl" parameterType="int" resultType="org.hyungwon.test.BoardVO">
        SELECT *
        FROM TB_BOARD
        <where>
            BOARD_SEQ = #{boardSeq}
        </where>
    </select>
</mapper>